{
  "extends": [
    "@dso-console/tsconfig/tsconfig.base.json"
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "declaration": true,
    "declarationDir": "./types",
    "lib": [
      "ESNext"
    ],
    "strict": false,
    "alwaysStrict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "coverage"
  ]
}