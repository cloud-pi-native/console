{
  "name": "client",
  "description": "dso-portal client",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 8080",
    "test:unit": "vitest --environment jsdom",
    "test:e2e-up": "docker-compose --file ../docker-compose.e2e.yml up --build",
    "test:e2e-down": "docker-compose --file ../docker-compose.e2e.yml down --volumes",
    "test:e2e": "npm run test:e2e-up; npm run test:e2e-down",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@gouvminint/vue-dsfr": "^1.5.0",
    "focus-trap": "^6.9.1",
    "focus-trap-vue": "^3.3.1",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.0.21",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.15",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/test-utils": "2.0.2",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.1.0",
    "vite-plugin-pwa": "^0.12.6",
    "vitest": "^0.23.1"
  }
}