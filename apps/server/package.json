{
  "name": "server",
  "version": "1.0.0",
  "description": "dso-portal server",
  "type": "module",
  "scripts": {
    "build": "run-s clean:build compile",
    "clean:build": "rimraf dist",
    "compile": "tspc || exit 0",
    "dev": "nodemon --exec 'vite-node src/server.ts'",
    "format": "eslint . --ext .js,.cjs,.ts --fix",
    "lint": "eslint . --ext .js,.cjs,.ts",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/session": "^10.3.0",
    "@gitbeaker/node": "^35.8.1",
    "@keycloak/keycloak-admin-client": "^21.1.1",
    "@kubernetes/client-node": "^0.18.1",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.17.0",
    "fastify-keycloak-adapter": "^2.0.1",
    "mustache": "^4.2.0",
    "nanoid": "4.0.2",
    "node-vault-client": "^0.6.1",
    "pg": "8.10.0",
    "sequelize": "^6.31.1",
    "shared": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.40.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "fastify-plugin": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "nodemon": "^2.0.22",
    "pino-pretty": "^10.0.0",
    "rimraf": "^5.0.0",
    "sequelize-mock": "^0.10.2",
    "test-utils": "workspace:^1.0.0",
    "ts-patch": "3.0.0-beta3",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.4",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^4.3.4",
    "vite-node": "^0.30.1",
    "vitest": "^0.30.1"
