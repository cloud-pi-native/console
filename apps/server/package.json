  "description": "dso-portal server",
  "devDependencies": {
    "@dso-console/eslint-config-base": "workspace:*",
    "@dso-console/test-utils": "workspace:*",
    "@dso-console/tsconfig": "workspace:*",
    "@faker-js/faker": "^8.4.0",
    "@kubernetes-models/argo-cd": "^2.4.0",
