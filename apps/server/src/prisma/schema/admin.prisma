model AdminPlugin {
  pluginName String
  key        String
  value      String

  @@unique([pluginName, key])
}

model AdminRole {
  id          String @id @unique @default(uuid()) @db.Uuid
  name        String
  permissions BigInt
  position    Int    @db.SmallInt
  oidcGroup   String @default("")
  type        String @default("custom")
}

model SystemSetting {
  key   String @id @unique
  value String
}
