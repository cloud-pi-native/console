{
  "name": "client",
  "version": "1.0.0",
  "description": "dso-portal client",
  "type": "module",
  "scripts": {
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "cypress:open": "cypress open",
    "cypress:open-ct": "export CT=true ; export NODE_ENV=test && cypress open --component",
    "cypress:run": "cypress run",
    "cypress:run-ct": "start-server-and-test preview http://localhost:8080 'cypress run --component --browser chrome'",
    "dev": "vite",
    "format": "eslint . --ext .js,.cjs,.ts,vue --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint": "run-p lint:*",
    "lint:styles": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "eslint . --ext .js,.cjs,.ts,.vue",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:ct": "run-s build test:ct:only",
    "test:ct:only": "start-server-and-test preview http://localhost:8080 'cypress run --component --browser chrome'",
    "test:e2e": "start-server-and-test preview http://localhost:8080 'cypress run --e2e --browser chrome'",
    "test:e2e:dev": "start-server-and-test 'vite dev' http://localhost:8080 'cypress open --e2e'",
    "test:unit": "run-s test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.9.1",
    "@gouvminint/vue-dsfr": "^3.4.2",
    "@vue/tsconfig": "^0.1.3",
    "axios": "^1.3.5",
    "keycloak-js": "^21.0.2",
    "jszip": "^3.10.1",
    "nanoid": "4.0.2",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.0.34",
    "shared": "workspace:^1.0.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.38.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "github-markdown-css": "^5.2.0",
    "jsdom": "^21.1.1",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "test-utils": "workspace:^1.0.0",
    "vite": "^4.2.1",
    "vite-plugin-md": "^0.21.5",
    "vite-plugin-windicss": "^1.8.10",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0",
    "windicss": "^3.5.6"
  },
  "optionalDependencies": {
    "@cypress/vue": "^5.0.5",
    "cypress": "^12.9.0"
  }
}
