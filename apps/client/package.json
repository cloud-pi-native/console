{
  "name": "@cpn-console/client",
  "version": "8.10.1",
  "description": "cpn-console client",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "run-s type-check build-only",
    "build-only": "vite --mode=production build",
    "build:clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "cypress:install": "cypress install",
    "dev": "vite --mode=development",
    "format": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./ --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "integ": "vite --mode=integration",
    "kube:e2e": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress open",
    "lint": "run-p lint:*",
    "lint:style": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ct": "NODE_ENV=test cypress open --component --browser=firefox",
    "test:ct-ci": "NODE_ENV=test cypress run --component --browser=firefox",
    "test:e2e": "cypress open",
    "test:e2e-ci": "cypress run",
    "kube:e2e-ci": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress run",
    "type-check": "vue-tsc --noEmit -p tsconfig.json || exit 0"
  },
  "dependencies": {
    "@cpn-console/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.12.1",
    "@gouvminint/vue-dsfr": "^5.14.2",
    "@iconify-json/ri": "^1.1.21",
    "@ts-rest/core": "^3.45.2",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^25.0.1",
    "nanoid": "5.0.7",
    "oh-vue-icons": "1.0.0-rc3",
    "p-debounce": "^4.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@cpn-console/eslint-config": "workspace:^",
    "@cpn-console/test-utils": "workspace:^",
    "@cpn-console/ts-config": "workspace:^",
    "@eslint/compat": "^1.1.0",
    "@faker-js/faker": "^8.4.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.9",
    "@unocss/transformer-directives": "^0.61.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.5.2",
    "unocss": "^0.61.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.2",
    "vitest": "^1.6.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.22"
  },
  "optionalDependencies": {
    "@cypress/vue": "^6.0.1",
    "cypress": "^13.12.0",
    "cypress-vite": "^1.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  }
}
