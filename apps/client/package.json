{
  "name": "@cpn-console/client",
  "type": "module",
  "version": "8.12.0",
  "private": false,
  "description": "cpn-console client",
  "scripts": {
    "build": "run-s type-check build-only",
    "build-only": "vite --mode=production build",
    "build:clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "cypress:install": "cypress install",
    "dev": "vite --mode=development",
    "format": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./ --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "integ": "vite --mode=integration",
    "kube:e2e": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress open",
    "lint": "run-p lint:*",
    "lint:style": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ct": "NODE_ENV=test cypress open --component --browser=firefox",
    "test:ct-ci": "NODE_ENV=test cypress run --component --browser=firefox",
    "test:e2e": "cypress open",
    "test:e2e-ci": "cypress run",
    "kube:e2e-ci": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress run",
    "type-check": "vue-tsc --noEmit -p tsconfig.json || exit 0"
  },
  "dependencies": {
    "@cpn-console/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.12.1",
    "@gouvminint/vue-dsfr": "^7.0.0",
    "@iconify/vue": "^4.1.2",
    "@ts-rest/core": "^3.51.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^25.0.5",
    "nanoid": "5.0.7",
    "p-debounce": "^4.0.0",
    "pinia": "^2.2.2",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5",
    "vue3-json-viewer": "^2.2.2"
  },
  "optionalDependencies": {
    "@cypress/vue": "^6.0.1",
    "cypress": "^13.14.2",
    "cypress-vite": "^1.5.0"
  },
  "devDependencies": {
    "@cpn-console/eslint-config": "workspace:^",
    "@cpn-console/test-utils": "workspace:^",
    "@cpn-console/ts-config": "workspace:^",
    "@eslint/compat": "^1.1.1",
    "@faker-js/faker": "^9.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.5",
    "@unocss/transformer-directives": "^0.62.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^2.1.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.6.2",
    "unocss": "^0.62.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.6",
    "vitest": "^2.1.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  }
}
