{
  "name": "@dso-console/client",
  "version": "6.5.2",
  "description": "dso-portal client",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "run-s clean:build type-check build-only",
    "build-only": "vite build",
    "clean:build": "rimraf dist",
    "cypress:install": "cypress install",
    "dev": "vite",
    "format": "eslint . --ext .js,.cjs,.ts,vue --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint": "run-p lint:*",
    "lint:styles": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "eslint . --ext .js,.cjs,.ts,.vue",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ct": "export CT=true export NODE_ENV=test && cypress open --component",
    "test:ct-ci": "export CT=true export NODE_ENV=test && cypress run --component",
    "test:e2e": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress open",
    "test:e2e-ci": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress run",
    "type-check": "vue-tsc --noEmit -p tsconfig.json || exit 0"
  },
  "dependencies": {
    "@dso-console/shared": "workspace:*",
    "@gouvfr/dsfr": "^1.11.0",
    "@gouvminint/vue-dsfr": "^5.7.0",
    "@vue/tsconfig": "^0.5.1",
    "axios": "1.6.4",
    "json-schema-to-ts": "^3.0.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^23.0.4",
    "nanoid": "5.0.2",
    "oh-vue-icons": "^1.0.0-rc3",
    "p-debounce": "^4.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@dso-console/eslint-config-base": "workspace:*",
    "@dso-console/test-utils": "workspace:*",
    "@rushstack/eslint-patch": "^1.6.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.4",
    "@unocss/transformer-directives": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.20.1",
    "@faker-js/faker": "^8.3.1",
    "jsdom": "^23.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^16.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "vue-tsc": "^1.8.27"
  },
  "optionalDependencies": {
    "@cypress/vue": "^6.0.0",
    "cypress": "^13.6.2",
    "cypress-vite": "^1.5.0"
  }
}
