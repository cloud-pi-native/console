{
  "name": "@cpn-console/client",
  "type": "module",
  "version": "9.3.3",
  "private": false,
  "description": "cpn-console client",
  "scripts": {
    "build": "run-s icons type-check build-only",
    "build-only": "vite --mode=production build",
    "build:clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "cypress:install": "cypress install",
    "dev": "vite --mode=development",
    "format": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./ --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "icons": "vue-dsfr-icons -s scripts/icons.js -t src/icon-collections.ts",
    "integ": "vite --mode=integration",
    "kube:e2e": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress open",
    "lint": "run-p lint:*",
    "lint:style": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' eslint ./",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ct": "NODE_ENV=test cypress open --component --browser=firefox",
    "test:ct-ci": "NODE_ENV=test cypress run --component --browser=firefox",
    "test:e2e": "cypress open --browser=firefox",
    "test:e2e-ci": "cypress run --browser=firefox",
    "kube:e2e-ci": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress run",
    "type-check": "vue-tsc --noEmit -p tsconfig.json || exit 0"
  },
  "dependencies": {
    "@cpn-console/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.13.0",
    "@gouvminint/vue-dsfr": "^8.2.1",
    "@iconify-json/ri": "^1.2.2",
    "@iconify/vue": "^4.2.0",
    "@ts-rest/core": "^3.51.0",
    "@vue/tsconfig": "^0.7.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "javascript-time-ago": "^2.5.11",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^26.0.7",
    "nanoid": "5.0.9",
    "p-debounce": "^4.0.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-json-viewer": "^2.2.2",
    "xbytes": "^1.9.1"
  },
  "optionalDependencies": {
    "@cypress/vue": "6.0.2",
    "cypress": "14.1.0",
    "cypress-vite": "1.6.0"
  },
  "devDependencies": {
    "@cpn-console/eslint-config": "workspace:^",
    "@cpn-console/test-utils": "workspace:^",
    "@cpn-console/ts-config": "workspace:^",
    "@eslint/compat": "^1.2.4",
    "@faker-js/faker": "^9.3.0",
    "@iconify/types": "^2.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@unocss/transformer-directives": "^0.65.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/eslint-config-typescript": "^14.1.4",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "stylelint": "^16.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "typescript": "^5.7.2",
    "unocss": "^0.65.1",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.3",
    "vite-node": "^2.1.8",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^2.1.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0",
    "workbox-window": "^7.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  }
}
