{
  "name": "client",
  "version": "1.0.0",
  "description": "dso-portal client",
  "type": "module",
  "scripts": {
    "build": "run-s type-check build-only",
    "build-only": "vite build",
    "clean:build": "rimraf dist",
    "cypress:open": "cypress open",
    "cypress:open-ct": "export CT=true ; export NODE_ENV=test && cypress open --component",
    "cypress:run": "cypress run",
    "cypress:run-ct": "cypress run --component",
    "dev": "vite",
    "format": "eslint . --ext .js,.cjs,.ts,vue --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint": "run-p lint:*",
    "lint:styles": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "eslint . --ext .js,.cjs,.ts,.vue",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false || exit 0"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.10.0-rc.1",
    "@gouvminint/vue-dsfr": "^4.0.2",
    "@vue/tsconfig": "^0.4.0",
    "axios": "^1.4.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^21.1.2",
    "nanoid": "4.0.2",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.1.4",
    "shared": "workspace:^",
    "typescript": "^5.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/jsdom": "^21.1.1",
    "@types/markdown-it-emoji": "^2.0.2",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.16.16",
    "@unocss/transformer-directives": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.32.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "cypress-vite": "^1.4.0",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "github-markdown-css": "^5.2.0",
    "jsdom": "^22.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.10.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^34.0.0",
    "test-utils": "workspace:^1.0.0",
    "unocss": "^0.53.4",
    "vite": "^4.3.9",
    "vite-plugin-md": "^0.21.5",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.8.3"
  },
  "optionalDependencies": {
    "@cypress/vue": "^5.0.5",
    "cypress": "^12.16.0"
  }
}