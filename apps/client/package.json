{
  "name": "@cpn-console/client",
  "version": "8.5.0",
  "description": "dso-portal client",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "run-s type-check build-only",
    "build-only": "vite --mode=production build",
    "build:clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "cypress:install": "cypress install",
    "dev": "vite --mode=development",
    "format": "eslint . --ext .js,.cjs,.ts,vue --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "integ": "vite --mode=integration",
    "kube:e2e": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress open",
    "lint": "run-p lint:*",
    "lint:styles": "stylelint ./src/**/*.{css,vue}",
    "lint:ts": "eslint . --ext .js,.cjs,.ts,.vue",
    "preview": "vite preview --port 8080",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:ct": "NODE_ENV=test cypress open --component",
    "test:ct-ci": "NODE_ENV=test cypress run --component",
    "test:e2e": "cypress open",
    "test:e2e-ci": "cypress run",
    "kube:e2e-ci": "CLIENT_HOST=console.dso.local CLIENT_PORT=80 cypress run",
    "type-check": "vue-tsc --noEmit -p tsconfig.json || exit 0"
  },
  "dependencies": {
    "@cpn-console/shared": "workspace:^",
    "@gouvfr/dsfr": "^1.11.2",
    "@gouvminint/vue-dsfr": "5.11.0",
    "@vue/tsconfig": "^0.5.1",
    "@iconify-json/ri": "^1.1.20",
    "@ts-rest/core": "^3.44.1",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "keycloak-js": "^24.0.3",
    "nanoid": "5.0.7",
    "oh-vue-icons": "^1.0.0-rc3",
    "p-debounce": "^4.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-router": "^4.3.2",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@cpn-console/eslint-config": "workspace:^",
    "@cpn-console/test-utils": "workspace:^",
    "@faker-js/faker": "^8.4.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@unocss/transformer-directives": "^0.59.4",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "cypress-vite": "^1.5.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-vue": "^9.25.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "unocss": "^0.60.4",
    "vite": "^5.2.10",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.13"
  },
  "optionalDependencies": {
    "@cypress/vue": "^6.0.0",
    "cypress": "^13.8.0",
    "cypress-vite": "^1.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "tag": "latest"
  }
}