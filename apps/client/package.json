{
  "name": "client",
  "description": "dso-portal client",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "cypress:open": "cypress open",
    "cypress:open-ct": "export CT=true ; export NODE_ENV=test && cypress open --component",
    "cypress:run": "cypress run",
    "cypress:run-ct": "cypress run --component",
    "dev": "vite",
    "format": "eslint . --ext .js,.cjs,.ts --fix",
    "format:style": "stylelint ./src/**/*.{css,vue} --fix",
    "lint": "eslint . --ext .js,.cjs,.ts && stylelint ./src/**/*.{css,vue}",
    "preview": "vite preview --port 8080",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@gouvminint/vue-dsfr": "^1.8.3",
    "axios": "^1.2.2",
    "keycloak-js": "^20.0.2",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.0.28",
    "shared": "workspace:^1.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.3",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.8.0",
    "github-markdown-css": "^5.1.0",
    "jsdom": "^21.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "nanoid": "4.0.0",
    "prism-theme-vars": "^0.2.4",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "test-utils": "workspace:^1.0.0",
    "vite": "^4.0.4",
    "vite-plugin-md": "^0.22.5",
    "vite-plugin-windicss": "^1.8.10",
    "vitest": "^0.26.3",
    "windicss": "^3.5.6"
  },
  "optionalDependencies": {
    "@cypress/vue": "^5.0.3",
    "cypress": "^12.3.0",
    "cypress-vite": "^1.3.0"
  }
