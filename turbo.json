{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:clean": {
      "dependsOn": [
        "^build:clean"
      ],
      "cache": false
    },
    "db:generate": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "format:style": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^@cpn-console/shared#build",
        "^@cpn-console/test-utils#build",
        "^@cpn-console/hooks#build",
        "^@cpn-console/argocd-plugin#build",
        "^@cpn-console/gitlab-plugin#build",
        "^@cpn-console/harbor-plugin#build",
        "^@cpn-console/keycloak-plugin#build",
        "^@cpn-console/kubernetes-plugin#build",
        "^@cpn-console/nexus-plugin#build",
        "^@cpn-console/sonarqube-plugin#build",
        "^@cpn-console/vault-plugin#build"
      ],
      "outputs": []
    },
    "test:cov": {
      "dependsOn": [
        "^@cpn-console/shared#build",
        "^@cpn-console/test-utils#build",
        "^@cpn-console/hooks#build",
        "^@cpn-console/argocd-plugin#build",
        "^@cpn-console/gitlab-plugin#build",
        "^@cpn-console/harbor-plugin#build",
        "^@cpn-console/keycloak-plugin#build",
        "^@cpn-console/kubernetes-plugin#build",
        "^@cpn-console/nexus-plugin#build",
        "^@cpn-console/sonarqube-plugin#build",
        "^@cpn-console/vault-plugin#build"
      ],
      "outputs": []
    },
    "test:ct": {
      "dependsOn": [
        "^@cpn-console/shared#build",
        "^@cpn-console/test-utils#build"
      ],
      "outputs": []
    },
    "test:ct-ci": {
      "dependsOn": [
        "^@cpn-console/shared#build",
        "^@cpn-console/test-utils#build"
      ],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": [
        "^@cpn-console/shared#build",
        "^@cpn-console/test-utils#build"
      ],
      "outputs": []
    },
    "test:e2e-ci": {
      "inputs": [
        "src/**"
      ],
      "outputs": []
    },
    "client#test:e2e-ci": {
      "dependsOn": [
        "@cpn-console/shared#test:e2e-ci",
        "@cpn-console/test-utils#test:e2e-ci",
        "server#test:e2e-ci"
      ],
      "inputs": [
        "src/**",
        "cypress/components/specs/**",
        "cypress/components/support/**",
        "cypress/e2e/specs/**",
        "cypress/e2e/support/**"
      ],
      "outputs": []
    },
    "kube:e2e-ci": {
      "inputs": [
        "src/**"
      ],
      "outputs": []
    },
    "client#kube:e2e-ci": {
      "dependsOn": [
        "@cpn-console/shared#kube:e2e-ci",
        "@cpn-console/test-utils#kube:e2e-ci",
        "server#kube:e2e-ci"
      ],
      "inputs": [
        "src/**",
        "cypress/components/specs/**",
        "cypress/components/support/**",
        "cypress/e2e/specs/**",
        "cypress/e2e/support/**"
      ],
      "outputs": []
    }
  }
}