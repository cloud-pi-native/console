{
  "name": "dso-console",
  "version": "2.2.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "prod": "npm run prod:up; npm run prod:down -v",
    "prod:build": "docker compose -f ./docker/docker-compose.prod.yml build --pull",
    "prod:up": "docker compose -f ./docker/docker-compose.prod.yml up",
    "prod:down": "docker compose -f ./docker/docker-compose.prod.yml down",
    "dev": "npm run dev:up; npm run dev:down",
    "dev:build": "docker compose -f ./docker/docker-compose.dev.yml build --pull",
    "dev:up": "docker compose -f ./docker/docker-compose.dev.yml up --attach server --attach client",
    "dev:down": "docker compose -f ./docker/docker-compose.dev.yml down",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "format:style": "turbo run format:style",
    "test": "turbo run test --concurrency 1",
    "test:client": "turbo run test --filter client",
    "test:server": "turbo run test --filter server",
    "test:shared": "turbo run test --filter shared",
    "test:ct": "pnpm --filter client run cypress:open-ct",
    "test:ct-ci": "docker compose -f ./docker/docker-compose.ct.yml up --exit-code-from cypress --quiet-pull; docker compose -f ./docker/docker-compose.ct.yml --env-file ./env/.env.ci down -v",
    "test:ct-ci-build": "docker compose -f ./docker/docker-compose.ct.yml build",
    "test:e2e": "docker compose -f ./docker/docker-compose.e2e.yml up -d; cd ./apps/client; npm run cypress:open; cd -; docker compose -f ./docker/docker-compose.e2e.yml down -v",
    "test:e2e-build": "docker compose -f ./docker/docker-compose.e2e.yml build --pull",
    "test:e2e-ci": "docker compose -f ./docker/docker-compose.ci.yml up --attach cypress --exit-code-from cypress --quiet-pull; docker compose -f ./docker/docker-compose.ci.yml down -v",
    "test:e2e-ci-build": "docker compose -f ./docker/docker-compose.ci.yml build --pull"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "turbo": "^1.8.1"
  },
  "volta": {
    "node": "18.14.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "sequelize-mock@0.10.2": "patches/sequelize-mock@0.10.2.patch"
    }
  }
}